. *************************************************************************************
. ******** Normalized poverty gap
. // Poverty gap is calculated as the difference between the poverty line and per capita income among those who are poor
. // Normalizing it means dividing it by the poverty line
. // Measures depth of poverty
. 
. *** Generate the normalized poverty gap of per capita income
. forvalues i = 1/2 {
  2.         local oldnew_name: word `i' of `oldnew'
  3.         qui gen pov_gap_`oldnew_name'2015 = poor_`oldnew_name'2015 * (1 - poor_`oldnew_name'2015_mult)
  4.         label variable pov_gap_`oldnew_name'2015 "Normalized poverty gap of pcinc (`oldnew_name' 2015 threshold)"
  5. }

. 
. *** Generate the normalized poverty gap variable net of health expenditures
. forvalues h = 1/3 {
  2.         local exp_name: word `h' of `exp_type'
  3.         local exp_title_name: word `h' of `exp_title'
  4.         forvalues i = 1/2 {
  5.                 local oldnew_name: word `i' of `oldnew'
  6.                 qui gen pov_gap_`oldnew_name'2015_`exp_name' = poor_`oldnew_name'2015_net_`exp_name' * (1 - diff_poor_`oldnew_name'2015_mult_`exp_name')
  7.                 label variable pov_gap_`oldnew_name'2015_`exp_name' "Norm pov gap of pcinc net of `exp_title_name' (`oldnew_name' 2015)"
  8.         }
  9. }

. 
. *** Generate the difference in poverty gaps and report differences
. forvalues h = 1/3 {
  2.         local exp_name: word `h' of `exp_type'
  3.         local exp_title_name: word `h' of `exp_title'
  4.         forvalues i = 1/2 {
  5.                 local oldnew_name: word `i' of `oldnew'
  6.                 qui gen absdiff_pov_gap_`oldnew_name'2015_`exp_name' = pov_gap_`oldnew_name'2015_`exp_name' - pov_gap_`oldnew_name'2015
  7.                 label variable absdiff_pov_gap_`oldnew_name'2015_`exp_name' "Abs diff of norm pov gap of pcinc net of `exp_title_name' (`oldnew_name' 2015)
> "
  8.                 svy: mean absdiff_pov_gap_`oldnew_name'2015_`exp_name', over(survey)
  9.                 qui gen reldiff_pov_gap_`oldnew_name'2015_`exp_name' = absdiff_pov_gap_`oldnew_name'2015_`exp_name' / pov_gap_`oldnew_name'2015
 10.                 label variable reldiff_pov_gap_`oldnew_name'2015_`exp_name' "Rel diff of norm pov gap of pcinc net of `exp_title_name' (`oldnew_name' 2015)
> "
 11.                 svy: mean reldiff_pov_gap_`oldnew_name'2015_`exp_name', over(survey)
 12.         }
 13. }
