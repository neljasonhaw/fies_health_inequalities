. ******** Concentration index
. // The concentration index can be estimated from the regression
. // 2*sigma_rank^2 (health / mean health) = constant + concentration index * rank + error
. // Retrieve the beta coefficient of rank to get the POINT estimate of the concentration index
. // For standard error, we may take the standard error of the beta coefficient but does not take account
. // the sampling variability of the estimate of the mean of the health variable.
. // To take into account the sampling variability of the mean of the health variable, use nlcom.
. // Consider also Newey-West regression to account for lag 1 autocorrelation as a function of the ranking
. 
. **** Generate a program that will calculate concentration indices
. capture program drop conc_index

. program define conc_index
  1.         syntax varlist (min = 4 max = 4) [fweight] using, year(integer)
  2.         use `using', clear
  3.         local outcome: word 1 of `varlist'
  4.         local rank: word 2 of `varlist'
  5.         local psu: word 3 of `varlist'
  6.         local prob_weight: word 4 of `varlist'
  7.         // Calculate the mean of the outcome and store in scalar mean
.         qui sum `outcome' [`weight'`exp']
  8.         local mean = r(mean)
  9.         // Calculate the variance of the rank and store in scalar v_rank
.         qui sum `rank' [`weight'`exp']
 10.         local v_rank = r(Var)
 11.         // Generate left hand side of regression
.         tempvar lhs 
 12.         gen `lhs' = 2 * `v_rank' * (`outcome'/`mean')
 13.         // Regress 
.         qui svy: regr `lhs' `rank'
 14.         // Retrieve coefficient - this is the concentration index point estimate
.         local c = _b[`rank']
 15.         local c_se = _se[`rank']
 16.         local c_upper = _b[`rank'] + abs(invnormal(0.025))*`c_se'
 17.         local c_lower = _b[`rank'] - abs(invnormal(0.025))*`c_se'
 18.         local c_pval = 1 - normal(`c'/`c_se')
 19.         // SE adjusted for variability of mean of health variable
.         qui svy: regr `outcome' `rank'
 20.         qui nlcom ((2 *`v_rank')/(_b[_cons] + 0.5* _b[`rank'])) * _b[`rank']
 21.         local c_se_adj = sqrt(r(V)[1,1])
 22.         local c_upper_adj = r(b)[1,1] + abs(invnormal(0.025))*`c_se_adj'
 23.         local c_lower_adj = r(b)[1,1] - abs(invnormal(0.025))*`c_se_adj'
 24.         local c_pval_adj = 1 - normal(r(b)[1,1]/`c_se_adj')
 25.         // Newey-West regression to account for serial correlation
.         tempvar ranki
 26.         egen `ranki' = rank(`rank'), unique
 27.         qui tsset `ranki'
 28.         qui newey `outcome' `rank' [aw = `prob_weight'], lag(1)
 29.         qui nlcom ((2 *`v_rank')/(_b[_cons] + 0.5* _b[`rank'])) * _b[`rank']
 30.         local c_se_newey = sqrt(r(V)[1,1])
 31.         local c_upper_newey = r(b)[1,1] + abs(invnormal(0.025))*`c_se_newey'
 32.         local c_lower_newey = r(b)[1,1] - abs(invnormal(0.025))*`c_se_newey'
 33.         local c_pval_newey = 1 - normal(r(b)[1,1]/`c_se_newey')
 34.         // Display all the results
.         di as text _dup(100) "="
 35.         di "The concentration index point estimate of `outcome' in `year' is " %6.5f `c'
 36.         di " "
 37.         di "Method" _col(50) "SE" _col(60)      "p-value" _col(70) "Lower 95% CI" _col(85) "Upper 95% CI"
 38.         di as text _dup(100) "-"
 39.         di "Unadjusted for variance of health variable" _col(50) %6.5f `c_se' _col(60) %4.3f `c_pval' _col(70) %6.5f `c_lower' _col(85) %6.5f `c_upper'
 40.         di "Adjusted for variance of health variable" _col(50) %6.5f `c_se_adj' _col(60) %4.3f `c_pval_adj' _col(70) %6.5f `c_lower_adj' _col(85) %6.5f `c_
> upper_adj'
 41.         di "Newey-West, autocorrelation lag (1)" _col(50) %6.5f `c_se_newey' _col(60) %4.3f `c_pval_newey' _col(70) %6.5f `c_lower_newey' _col(85) %6.5f `c
> _upper_newey'
 42.         di as text _dup(100) "="
 43. end

