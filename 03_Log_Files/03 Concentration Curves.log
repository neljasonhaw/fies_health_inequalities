. ******** Kakwani's progressivity index
. // The concentration index can be estimated from the regression
. // 2*sigma_rank^2 (health / mean health - income / mean income) = constant + Kakwani index * rank + error
. // Retrieve the beta coefficient of rank to get the POINT estimate of the concentration index
. // For standard error, we may take the standard error of the beta coefficient but does not take account
. // the sampling variability of the estimate of the mean of the health variable.
. 
. **** Generate a program that will calculate Kakwani's progressiviy indices
. capture program drop kakwani_index

. program define kakwani_index
  1.         syntax varlist (min = 5 max = 5) [fweight] using, year(integer)
  2.         use `using', clear      
  3.         local outcome: word 1 of `varlist'
  4.         local living: word 2 of `varlist' 
  5.         local rank: word 3 of `varlist'
  6.         local psu: word 4 of `varlist'
  7.         local prob_weight: word 5 of `varlist'
  8.         // Calculate the mean of the outcome and store in scalar mean
.         qui sum `outcome' [`weight'`exp']
  9.         local mean_outcome = r(mean)
 10.         // Calculate the mean of the living standards variable (income) and store in scalar mean
.         qui sum `living' [`weight'`exp']
 11.         local mean_living = r(mean)
 12.         // Calculate the variance of the rank and store in scalar v_rank
.         qui sum `rank' [`weight'`exp']
 13.         local v_rank = r(Var)
 14.         // Generate left hand side of regression
.         tempvar lhs 
 15.         gen `lhs' = 2 * `v_rank' * (`outcome'/`mean_outcome' - `living'/`mean_living')
 16.         // Regress 
.         qui svy: regr `lhs' `rank'
 17.         // Retrieve coefficient - this is the Kakwani progressivity index point estimate
.         local ki = _b[`rank']
 18.         local ki_se = _se[`rank']
 19.         local ki_upper = _b[`rank'] + abs(invnormal(0.025))*`ki_se'
 20.         local ki_lower = _b[`rank'] - abs(invnormal(0.025))*`ki_se'
 21.         local ki_pval = normal(`ki'/`ki_se')
 22.         // Regress to retrieve outcome concentration index
.         tempvar lhs_outcome
 23.         gen `lhs_outcome' = 2 * `v_rank' * (`outcome'/`mean_outcome')
 24.         qui svy: regr `lhs_outcome' `rank'
 25.         local ci_outcome = _b[`rank']
 26.         // Regress to retrieve living standards concentration index
.         tempvar lhs_living
 27.         gen `lhs_living' = 2 * `v_rank' * (`living'/`mean_living')
 28.         qui svy: regr `lhs_living' `rank'
 29.         local ci_living = _b[`rank']
 30.         // Display all the results
.         di as text _dup(110) "="
 31.         di "Kakwani Progressivity Index for `outcome'" " for the year " `year' " using ranking variable `living'"
 32.         di " "
 33.         di "CIndex Outcome" _col(18) "CIndex Living" _col(35) "Kakwani Index" _col(50) "SE" _col(60) "p-value" _col(70) "Lower 95% CI" _col(85) "Upper 95% 
> CI"
 34.         di as text _dup(100) "-"
 35.         di %6.5f `ci_outcome' _col(18) %6.5f `ci_living' _col(35) %6.5f `ki' _col(50) %6.5f `ki_se' _col(60) %4.3f `ki_pval' _col(70) %6.5f `ki_lower' _col
> (85) %6.5f `ki_upper'
 36.         di as text _dup(100) "="
 37. end

