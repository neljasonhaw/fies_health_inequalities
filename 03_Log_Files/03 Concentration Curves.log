. **** Sensitivity analysis using household expenditure
. // The regression is: 2*sigma_rerank^2 (health / mean health) = constant + gamma * rerank + error
. // where rerank = rank_1i - rank_2i and gamma is the difference in concentration indices (CI_1 - CI_2)
. // Rank 1 is income, Rank 2 is expenditure
. capture program drop conc_index_sensi

. program define conc_index_sensi
  1.         syntax varlist (min = 5 max = 5) [fweight] using/, year(integer)
  2.         use `using', clear
  3.         local outcome: word 1 of `varlist'
  4.         local rank1: word 2 of `varlist'
  5.         local rank2: word 3 of `varlist'
  6.         local psu: word 4 of `varlist'
  7.         local prob_weight: word 5 of `varlist'
  8.         // Calculate the mean of the outcome and store in scalar mean
.         qui sum `outcome' [`weight'`exp']
  9.         local mean = r(mean)
 10.         // Create a new variable rerank
.         tempvar rerank
 11.         gen `rerank' = `rank1' - `rank2'
 12.         // Calculate the variance of the rerank and store in scalar v_rerank
.         qui sum `rerank' [`weight'`exp']
 13.         local v_rerank = r(Var)
 14.         // Generate left hand side of regression
.         tempvar lhs 
 15.         gen `lhs' = 2 * `v_rerank' * (`outcome'/`mean')
 16.         // Regress 
.         qui svy: regr `lhs' `rerank'
 17.         local gamma_est = _b[`rerank']
 18.         local gamma_se = _se[`rerank']
 19.         // There is no transformation that accounts for the variability of the health variable
.         // If the p-value is too low anyway, any additional variability of the health variable will increase the p-value but not by much
.         local gamma_upper = `gamma_est' + abs(invnormal(0.025))*`gamma_se'
 20.         local gamma_lower = `gamma_est' - abs(invnormal(0.025))*`gamma_se'
 21.         local gamma_est_p = normal(`gamma_est'/`gamma_se')
 22.         // Generate the point estimates of the concentration indices
.         forvalues i = 1/2 {
 23.                 // Calculate the variance of rank and store in scalar v_rank
.                 qui sum `rank`i'' [`weight'`exp']
 24.                 local v_rank`i' = r(Var)
 25.                 // Generate left hand side of regression of rank
.                 tempvar lhs_rank`i'
 26.                 gen `lhs_rank`i'' = 2 * `v_rank`i'' * (`outcome'/`mean')
 27.                 // Regress and retrieve concentration index of rank
.                 qui svy: regr `lhs_rank`i'' `rank`i''
 28.                 local c_rank`i' = _b[`rank`i'']
 29.         }
 30.         // Display all the results
.         di as text _dup(100) "="
 31.         di "`outcome'" " for the year " `year'
 32.         di " "
 33.         di "CIndex Income" _col(20) "CIndex Expenditure" _col(40) "Diff" _col(50) "SE" _col(60) "p-value" _col(70) "Lower 95% CI" _col(85) "Upper 95% CI"
 34.         di as text _dup(100) "-"
 35.         di %6.5f `c_rank1' _col(20) %6.5f `c_rank2' _col(40) %6.5f `gamma_est' _col(50) %6.5f `gamma_se' _col(60) %4.3f `gamma_est_p' _col(70) %6.5f `gamma
> _lower' _col(85) `gamma_upper'
 36.         di as text _dup(100) "="
 37. end

